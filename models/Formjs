const mongoose = require("mongoose")

const formSchema = new mongoose.Schema({
    firstname:{
        type:String
    },
    email:{
        type:String,
    },
    address:{
        type:String,
    },
    person:{
        type:String,
    },
    month:{
        type:String,
    },
    message:{
        type:String,
    },
    lastname:{
        type:String
    },
    phone:{
        type:String
    },toaddress:{
        type:String
    },
    luggage:{
        type:String
    },journeytime:{
        type:String
    },
  
})

module.exports=mongoose.model('Form',formSchema,'Form')